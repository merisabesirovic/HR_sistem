<Window x:Class="HR_sistem.Views.Chart"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d"
        Title="Performance Review Chart" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="5*"/>
            <RowDefinition Height="*"/>

        </Grid.RowDefinitions>
        
        <Canvas Grid.Row="0" Name="ChartCanvas" Background="White"/>
        <TextBlock Grid.Row="1" Margin="20, 0" FontSize="12" Text="Score from reviews trough time"/>

    </Grid>
</Window>
